spring:
  application:
    name: eureka-ha
---
spring:
  profiles: eureka1
  security:
    user:
      name: admin
      password: 123123
server:
  port: 8761
eureka:
  server:
    enable-self-preservation: true #开启自我保护
    eviction-interval-timer-in-ms: 30000 # 配置eureka 清理没有续约的instance间隔（单位毫秒，默认是60*1000）
  instance:
    hostname: eureka1
    prefer-ip-address: true
    instance-id: ${spring.cloud.client.ip-address}:${server.port}:${spring.application.name}
  client:
    healthcheck:
      enabled: true
    serviceUrl:
      defaultZone: http://admin:123123@eureka2:8762/eureka/,http://admin:123123@eureka3:8763/eureka/
    fetch-registry: true
    register-with-eureka: true
